{
  "name": "inventoryapp",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.4.2",
    "npm": "8.5.0"
  },
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "server": "nodemon ./server.js",
    "build": "cd frontend/inventory-app && npm run build",
    "install-client": "cd frontend/inventory-app && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \" cd frontend/inventory-app && npm run start\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^7.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "express-jwt": "^7.7.0",
    "express-openid-connect": "^2.7.2",
    "express-router": "^0.0.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.1",
    "mongoose": "^6.3.2",
    "morgan": "~1.9.1",
    "nodemailer": "^7.0.3",
    "nodemon": "^2.0.16",
    "passport": "^0.5.2",
    "passport-openidconnect": "^0.1.1"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^3.0.1",
    "jest": "^28.1.1"
  }
}
